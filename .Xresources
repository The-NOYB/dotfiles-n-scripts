!!! urxvt-256color
!
urxvt-256color.font:xft:Terminus (TTF)-Bold:pixelsize=21
urxvt-256color.letterspace:1 
urxvt-256color.scrollBar:	false
urxvt-256color.saveLines:	4096
urxvt-256color.perl-ext-common: default,matcher,keyboard-select,resize-font,url-select,clipboard
urxvt-256color.keysym.M-Escape:   perl:keyboard-select:activate
urxvt-256color.keysym.M-c:   perl:clipboard:copy
urxvt-256color.keysym.M-v:   perl:clipboard:paste
urxvt-256color.keysym.M-u: perl:url-select:select_next
urxvt-256color.url-select.launcher: /usr/bin/xdg-open
urxvt-256color.url-select.underline: true
urxvt-256color.matcher.button: 1
Xft*dpi:       100
Xft*antialias: True
Xft*hinting:   True
Xft*rgba: rgb
urxvt-256color.keysym.KP_Add:    +
urxvt-256color.keysym.KP_Subtract: -
urxvt-256color.keysym.KP_Multiply: *
urxvt-256color.keysym.KP_Divide  : /
urxvt-256color.keysym.KP_Decimal : .
urxvt-256color.keysym.KP_Separator : ,
urxvt-256color.keysym.KP_1:      1
urxvt-256color.keysym.KP_2:      2
urxvt-256color.keysym.KP_3:      3
urxvt-256color.keysym.KP_4:      4
urxvt-256color.keysym.KP_5:      5
urxvt-256color.keysym.KP_6:      6
urxvt-256color.keysym.KP_8:      8
urxvt-256color.keysym.KP_7:      7
urxvt-256color.keysym.KP_9:      9
urxvt-256color.keysym.KP_0:      0
urxvt-256color.geometry:75x20
urxvt-256color.keysym.C-j:resize-font:smaller
urxvt-256color.keysym.C-k:resize-font:bigger
urxvt-256color.keysym.C-equal:resize-font:reset
urxvt-256color.preeditType:Root
urxvt-256color.keysym.M-l:reset
urxvt-256color.keysym.M-f:ranger
urxvt-256color.keysym.M-m:mount_drives

